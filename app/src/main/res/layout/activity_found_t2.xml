<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#8E8E8E"
    tools:context=".FoundT2">

    <ImageView
        android:id="@+id/view7"
        android:layout_width="289dp"
        android:layout_height="374dp"
        android:layout_centerInParent="true"
        app:srcCompat="@drawable/box_outside" />

    <ImageView
        android:id="@+id/imageView23"
        android:layout_width="wrap_content"
        android:layout_height="300dp"
        android:layout_alignTop="@+id/view7"
        android:layout_centerHorizontal="true"
        app:srcCompat="@drawable/found_t2" />

    <Button
        android:id="@+id/button10"
        android:layout_width="125dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/imageView23"
        android:layout_marginStart="47dp"
        android:background="@color/colorbtnmasuk"
        android:onClick="batal"
        android:text="Batal"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="15sp"
        android:textStyle="bold"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="47dp" />

    <Button
        android:id="@+id/button11"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/imageView23"
        android:layout_marginEnd="48dp"
        android:background="@color/colorPrimaryDark"
        android:onClick="Next"
        android:text="Selesai"
        android:textColor="@color/colorbtnmasuk"
        android:textSize="15sp"
        android:textStyle="bold"
        android:layout_alignParentRight="true"
        android:layout_marginRight="48dp" />

</RelativeLayout>